      <add assembly="NLog.Web.AspNetCore"/>
    </extensions>
    <targets>
      <target xsi:type="File" name="filedata" fileName="${basedir}/logs/${shortdate}.log" layout="${longdate} ${uppercase:${level}} ${message}"/>
    </targets>
    <rules>
      <logger name="*" minlevel="Debug" writeTo="filedata"/>
    </rules>
  </nlog>

